extends layout

block content
    h1= title
    each song in songs
        a(href='/' + song._id,style='outline: 1px solid; padding: 10px; margin: 10px; display: inline-block;')
            p Title: #{song.title}
            p Artist: #{song.artist}
    p
        a.btn.btn-large.btn-primary(href='/new') New song
